{
  "name": "psyfactory/oauth2-softrig",
  "description": "SoftRig OAuth 2.0 support for the PHP League's OAuth 2.0 Client",
  "authors": [
      {
          "name": "Kees Brandenburg",
          "email": "sales@psyfactory.nl"
      }
  ],
  "keywords": [
    "php",
    "softrig",
    "api",
    "oauth",
    "oauth2"
  ],
  "require": {
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Psyfactory\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Psyfactory\\OAuth2\\Client\\Test\\": "test/src/"
    }
  }
}
